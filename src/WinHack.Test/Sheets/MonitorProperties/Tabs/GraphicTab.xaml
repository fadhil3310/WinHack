<UserControl x:Class="WinHack.Test.Sheets.MonitorProperties.Tabs.GraphicTab"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WinHack.Test.Sheets.MonitorProperties.Tabs"
             mc:Ignorable="d" 
             x:Name="root"
             DataContext="{Binding RelativeSource={RelativeSource Self}}"
             d:DesignHeight="268" d:DesignWidth="208"
             d:Background="White">
    <ScrollViewer VerticalScrollBarVisibility="Hidden">
								<StackPanel>
												<GroupBox Header="Animation Test">
																<Button Command="{Binding AnimMotionBlurCommand}">Motion Blur</Button>
												</GroupBox>
								</StackPanel>
								</ScrollViewer>
</UserControl>
